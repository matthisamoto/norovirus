<div id="user_list">
<div id="users">
<% @list.each do |l| %>
  <% if l.name != "" %>
  <div id="<%= l.id %>" class="user">
	<div class="name"><%= l.name %> <% if l.location != "" %> <span class="location"><%= l.location %></span><% end %></div>
	<% if l.story != "" %>
	<div class="story hidden"><%= l.story %></div>
  	<% end %>
  </div>
  <% end %>
<% end %>
</div>
</div>